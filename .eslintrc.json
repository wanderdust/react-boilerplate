{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true,
    "jest": true
  },
  "plugins": [
    "react-hooks"
  ],
  "rules": {
    "comma-dangle": [
      "error",
      "never"
    ],
    "no-unused-vars": [
      "error",
      {
        "vars": "local",
        "args": "none"
      }
    ],
    "react/jsx-filename-extension": 0,
    "react-hooks/rules-of-hooks": "error",
    "no-underscore-dangle": "off",
    "import/no-named-as-default": 0,
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "react/prop-types": "warn"
  }
}